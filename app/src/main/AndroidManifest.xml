<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.fashkl.project">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/train1"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".AddBalancePage"
                android:label="Booking"
        />
        <activity
                android:name=".BookTicketPage"
                android:label="Booking"
        />
        <activity
                android:name=".SelectPage"
                android:label="Details"/>
        <activity
                android:name=".LoginPage"
                android:label="Login"
        />
        <activity
                android:name=".ViewTicketPage"
                android:label="Search Ticket"
        />
        <activity
                android:name=".TicketDetailsPage"
                android:label="Ticket Details"
        />

        <activity
                android:name=".RegisterPage"
                android:label="Registration"></activity>
    </application>

</manifest>